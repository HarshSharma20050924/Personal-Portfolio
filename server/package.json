{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/server.mjs",
  "scripts": {
    "start": "node src/server.mjs",
    "dev": "node --watch src/server.mjs",
    "build": "npx prisma generate",
    "postinstall": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "@vercel/blob": "^0.23.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "prisma": "^5.15.0"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs",
    "schema": "prisma/schema.prisma"
  }
}